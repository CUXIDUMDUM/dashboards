  <li class="row-1 col-3">
    <div data-id="members" data-view="Number" data-unordered="true" data-title="<%= t 'commercial.members' %>" id="members"></div>
  </li>
  <li class="row-2 col-3">
    <div data-id="age" data-view="Number" data-unordered="true" data-title="<%= t 'commercial.opportunity_age' %>" id="age"></div>
  </li>
  <li class="row-3 col-3">
    <div data-id="old-opportunities" data-view="Number" data-unordered="true" data-title="<%= t 'commercial.old_opportunities' %>" id="old-opportunities"></div>
  </li>
</ul>
