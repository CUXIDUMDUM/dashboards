<% content_for(:title) { "ODI Company Dashboard" } %>
<div class="gridster" id="company">
  <ul>
    <li data-row="1" data-col="1" data-sizex="8" data-sizey="1" id="company-header">
        <a href="http://theodi.org/"><img src="/assets/companylogo.png" /></a>
        <h1 class="title">2014 Company Dashboard</h1>
    </li>
    <li data-row="2" data-col="1" data-sizex="2" data-sizey="2">
      <div data-id="2014-Reach" data-view="Number" data-unordered="true" data-title="People reached" id="reach"></div>
    </li>
    <li data-row="2" data-col="3" data-sizex="2" data-sizey="2">
      <div data-id="2014-Value" data-view="Number" data-unordered="true" data-title="Value unlocked" id="value"></div>
    </li>
    <li data-row="2" data-col="5" data-sizex="2" data-sizey="2">
      <div data-id="2014-network-size" data-view="NumberWithTarget" data-unordered="true" data-title="Network size" id="network"></div>
    </li>
    <li data-row="4" data-col="1" data-sizex="2" data-sizey="2">
      <div data-id="2014-commercial-bookings" data-view="NumberWithTarget" data-unordered="true" data-title="Commercial bookings" id="bookings"></div>
    </li>
    <li data-row="4" data-col="3" data-sizex="2" data-sizey="2">
      <div data-id="2014-non-commercial-bookings" data-view="NumberWithTarget" data-unordered="true" data-title="Non-commercial bookings" id="non-commercial-bookings"></div>
    </li>
    <li data-row="4" data-col="5" data-sizex="2" data-sizey="2">
      <div data-id="2014-grant-funding" data-view="NumberWithTarget" data-unordered="true" data-title="Grant Funding" id="grant-funding"></div>
    </li>
    <li data-row="2" data-col="7" data-sizex="2" data-sizey="4">
      <div data-id="2014-revenue-by-sector" data-view="Pie" data-title="Revenue by sector" id="revenue-by-sector"></div>
    </li>
  </ul>
</div>
