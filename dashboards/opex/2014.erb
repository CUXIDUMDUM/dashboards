<ul class="grid rows-6 cols-4" id="opex">
  <li class="row-1 col-1 height-3">
    <div data-id="2014-Income" data-view="NumberWithTarget" data-unordered="true" data-title="<%= t 'opex.income' %>" id="income" data-tooltip="<%= t 'opex.tips.income' %>"></div>
  </li>
  <li class="row-4 col-1 height-3">
    <div data-id="2014-Total-Costs" data-view="NumberWithTarget" data-unordered="true" data-title="<%= t 'opex.costs' %>" id="total-costs" data-tooltip="<%= t 'opex.tips.costs' %>"></div>
  </li>
  <li class="row-1 col-2 height-2">
    <div data-id="2014-Headcount" data-view="Number" data-unordered="true" data-title="<%= t 'opex.headcount' %>" id="headcount" data-tooltip="<%= t 'opex.tips.headcount' %>"></div>
  </li>
  <li class="row-3 col-2 height-2">
    <div data-id="2014-EBITDA-YTD" data-view="Number" data-unordered="true" data-title="<%= t 'opex.ebitda_ytd' %>" id="ebitda-ytd" data-tooltip="<%= t 'opex.tips.ebitda_ytd' %>"></div>
  </li>
  <li class="row-5 col-2 height-2">
    <div data-id="2014-EBITDA" data-view="Number" data-unordered="true" data-title="<%= t 'opex.ebitda_month' %>" id="ebitda-month" data-tooltip="<%= t 'opex.tips.ebitda_month' %>"></div>
  </li>
  <li class="row-1 col-3 height-3">
    <div data-id="2014-Burn" data-view="Number" data-unordered="true" data-title="<%= t 'opex.burn' %>" id="burn" data-tooltip="<%= t 'opex.tips.burn' %>"></div>
  </li>
  <li class="row-4 col-3 height-3">
    <div data-id="2014-Cash-Reserves" data-view="Number" data-unordered="true" data-title="<%= t 'opex.cash' %>" id="cash" data-tooltip="<%= t 'opex.tips.cash' %>"></div>
  </li>
  <li class="row-1 col-4 height-6">
    <div data-id="2014-Fixed-cost-breakdown" data-view="Pie" data-title="<%= t 'opex.costs' %>" id="costs" data-tooltip="<%= t 'opex.tips.costs' %>"></div>
  </li>
</ul>
