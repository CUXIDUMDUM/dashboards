<ul class="grid rows-4 cols-11" id="progress">
  <li class="row-1 col-1 width-3">
    <div data-id="<%= @year %>-q4-progress" data-view="Smallmeter" data-unordered="true" data-title="Q4 progress" id="q4-progress"></div>
  </li>
  <li class="row-2 col-1 width-3">
    <div data-id="<%= @year %>-q3-progress" data-view="Smallmeter" data-unordered="true" data-title="Q3 progress" id="q3-progress"></div>
  </li>
  <li class="row-3 col-1 width-3">
    <div data-id="<%= @year %>-q2-progress" data-view="Smallmeter" data-unordered="true" data-title="Q2 progress" id="q2-progress"></div>
  </li>
  <li class="row-4 col-1 width-3">
    <div data-id="<%= @year %>-q1-progress" data-view="Smallmeter" data-unordered="true" data-title="Q1 progress" id="q1-progress"></div>
  </li>
  <li class="row-1 col-4 width-4 height-4">
    <div data-id="<%= @year %>-<%= @quarter %>-rest-of-quarter" data-view="ProgressList" data-title="Outstanding Tasks" id="this-month"></div>
  </li>
  <li class="row-1 col-8 width-2 height-4">
    <div data-id="<%= @year %>-<%= @quarter %>-discuss" data-view="ProgressList" data-title="To discuss" id="discuss"></div>
  </li>
  <li class="row-1 col-10 width-2 height-4">
    <div data-id="<%= @year %>-<%= @quarter %>-done" data-view="ProgressList" data-title="Done" id="done"></div>
  </li>
</ul>
